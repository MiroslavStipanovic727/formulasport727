<html t:type="layout" title="Admin Panel"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <t:zone t:id="zonaFormeIzborPodataka">
        <t:form t:id="izborPodataka" t:zone="zonaPrikazaIzabranihPodataka">
            <t:label for="tipovi"/>
            <t:select t:id="tipovi" label="message:izborTipa-label"
            onchange="this.form.submit()"/>
        </t:form>
    </t:zone>
    
    <t:zone t:id="zonaPrikazaIzabranihPodataka">
        <t:if test="prikazKlijenata">
            <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-3">  
                    <t:zone t:id="zonaFormeUnosIzmenaKorisnika"> 
                        <t:if test="imaKorisnika">
                            <t:zone t:id="zonaFormeIzmenaKorisnika">  
                                <t:beaneditform t:id="izmenakorisnika" object="korisnik" exclude="id" 
                                                reorder="email,sifra,izmenasifre,imeKorisnika,prezimeKorisnika,uloga"
                                                add="kid, izmenasifre" submitlabel="message:submit-label-korisnik-izmena">
                                    <p:kid>
                                        <t:label for="omiljenaKategorija"/>:
                                        <t:select t:blankLabel="Omiljena Kategorija" t:id="omiljenaKategorija" 
                                                  encoder="encoderKategorije" model="kategorije"/>
                                    </p:kid>
                                    <p:izmenasifre>
                                        <t:label for="izmenaSifre"/>:
                                        <t:Checkbox t:id="izmenaSifre" type="checkbox"/>
                                    </p:izmenasifre>
                                    <t:errors/>            
                                </t:beaneditform> 
                            </t:zone>
                            <t:actionlink t:id="resetKorisnika" t:zone="zonaFormeUnosIzmenaKorisnika" 
                                          class="btn btn-primary btn-large">${message:reset-label}</t:actionlink>   
                        </t:if> 
                        <t:if test="!imaKorisnika">
                            <t:zone t:id="zonaFormeUnosKorisnika">
                                <t:beaneditform t:id="unoskorisnika" object="korisnik" exclude="id"
                                            reorder="email,sifra, imeKorisnika,prezimeKorisnika,uloga"
                                            add="kid" submitlabel="message:submit-label-korisnik">
                                <p:kid>
                                    <t:label for="omiljenaKategorijaUnos"/>:
                                    <t:select t:blankLabel="Omiljena Kategorija" t:id="omiljenaKategorijaUnos" 
                                              encoder="encoderKategorije" model="kategorije"/>
                                </p:kid>
                                <t:errors/>            
                                </t:beaneditform>  
                            </t:zone>      
                        </t:if>
                    </t:zone>
                </div>
                <div class="col-sm-8 col-md-8 col-lg-9">
                    <t:zone t:id="zonaGridaKorisnik"> 
                        <t:jquery.datatable t:source="korisnici" t:options="options" 
                                            add="editkorisnik, deletekorisnik, kategorija" row="redkorisnik"
                                exclude="id" reorder="korisnickoIme,email,sifra,imeKorisnika,prezimeKorisnika,lokacija,uloga,kategorija">
                            <p:editkorisnikCell>
                                <t:actionlink t:id="editkorisnik" t:zone="zonaFormeIzmenaKorisnika" 
                                              context="redkorisnik">${message:izmeni}</t:actionlink>
                            </p:editkorisnikCell>
                            <p:deletekorisnikCell>
                                <t:actionlink t:id="deletekorisnik" context="redkorisnik.email" 
                                              t:zone="zonaGridaKorisnik"
                                              t:title="message:potvrdaBrisanja-label" 
                                              t:message="${message:potvrdaBrisanjaKorisnika-label}
                                              ${redkorisnik.korisnickoIme}" t:mixins="Confirm">${message:obrisi}</t:actionlink>
                            </p:deletekorisnikCell>
                            <p:kategorijaCell>
                                ${OmiljenaKategorijaTabela}
                            </p:kategorijaCell>
                        </t:jquery.datatable>
                    </t:zone>
                </div>
            </div> 
        </t:if>
        <t:if test="prikazArtikala">
            <div class="row">

                <div class="col-sm-4 col-md-4 col-lg-3">  
                    <t:zone t:id="zonaFormeUnosArtikla">
                        <t:beaneditform t:id="unosIzmenaArtikla" object="artikal" exclude="id"
                                        reorder="nazivArtikla, kratakOpis, cena"
                                        add="tid, novtip" submitlabel="message:submit-label-artikal">
                            <p:tid>
                                <t:label for="kategorijaArtikla"/>:
                                <t:select t:blankLabel="Kategorija Artikla" t:id="kategorijaArtikla" 
                                          encoder="encoderTipaArtikla" model="tipoviArtikla"/>
                            </p:tid>
                            <p:novtip>
                                <t:label for="novtip"/>:
                                <t:Textfield t:id="novtip"/>
                            </p:novtip>
                            <t:errors/>            
                        </t:beaneditform> 
                    </t:zone>
                    <t:actionlink t:id="resetArtikla" class="btn btn-primary btn-large" 
                    t:zone="zonaFormeUnosArtikla">
                        ${message:reset-label}
                    </t:actionlink>   
                </div>
                <div class="col-sm-8 col-md-8 col-lg-9"> 
                    <t:zone t:id="zonaFormeTipArtikla">
                        <t:form t:id="izborTipaArtikla" t:zone="zonaPrikazaArtikala">
                            <t:label for="izborKategorijeArtikla"/>    
                            <t:select t:id="izborKategorijeArtikla" t:model="kategorijeArtiklaTabela" 
                                      onchange="this.form.submit()" encoder="encoderTipaArtikla" 
                                      t:value="kategorijaArtiklaTabela" label="message:izborKategorijeArtikla-label"/>
                        </t:form>
                    </t:zone>
                    <t:zone t:id="zonaPrikazaArtikala">
                        <t:jquery.datatable t:source="artikli" t:options="options" add="editartikal, deleteartikal, tipartikla" row="redartikal"
                                exclude="id" reorder="nazivArtikla, kratakOpis, cena, tipartikla, editartikal, deleteartikal">-->   
                            <p:editartikalCell>
                                <t:actionlink t:id="editartikal" context="redartikal"
                                t:zone="zonaFormeUnosArtikla">
                                    ${message:izmeni}
                                </t:actionlink>
                            </p:editartikalCell>
                            <p:deleteartikalCell>
                                <t:actionlink t:id="deleteartikal" context="redartikal.id" 
                                t:title="message:potvrdaBrisanja-label" t:zone="zonaPrikazaArtikala"
                                t:message="${message:potvrdaBrisanjaArtikla-label}
                                ${redartikal.nazivArtikla}" t:mixins="Confirm">
                                    ${message:obrisi}</t:actionlink>
                            </p:deleteartikalCell>
                            <p:tipartiklaCell>
                                ${TipArtikla}
                            </p:tipartiklaCell>
                        </t:jquery.datatable>
                    </t:zone>
                </div>
            </div>
        </t:if>
        <t:if test="prikazSampionata">
            <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-3"> 
                    <t:zone t:id="zonaFormeIzborPodatakaUnos"> 
                        <t:form t:id="izborPodatakaUnos" t:zone="zonaPrikazaUnosFormeSampionata">
                            <t:label for="izborKategorijeUnos"/>    
                            <t:select t:id="izborKategorijeUnos" t:model="kategorijeUnos" onchange="this.form.submit()"
                                      encoder="encoderKategorije" t:value="vrednostKategorijeUnos" label="message:izborKategorije-label"/>
                            <t:label for="novaKategorijaUnos"/>    
                            <t:Textfield t:id="novaKategorijaUnos"/>
                            <t:label for="izborSezoneUnos"/>
                            <t:select t:id="izborSezoneUnos" t:model="sezoneUnos" onchange="this.form.submit()"
                                      encoder="encoderSezone" t:value="vrednostSezoneUnos" label="message:izborSezone-label"/>
                            <t:label for="novaSezonaUnos"/>    
                            <t:Textfield t:id="novaSezonaUnos"/>
                            <t:label for="tipovi"/>
                            <t:select t:id="tipoviPodatakaUnos" label="message:izborTipa-label" onchange="this.form.submit()"/>
                            <t:submit class="btn btn-large btn-primary" value="message:izaberi-podatke-label"/>
                        </t:form>
                    </t:zone>
                    <t:if test="prikazUnosForme">
                        <t:zone t:id="zonaPrikazaUnosFormeSampionata">
                            <t:if test="UnosFormaVozac">
                                <t:zone t:id="zonaFormeUnosIzmenaVozaca">
                                    <t:beaneditform t:id="unosIzmenaVozaca" object="vozac" exclude="id"
                                                    reorder="imeVozaca, prezimeVozaca, drzid, timid, broj, datumRodjenja, slika, pozicija, bodovi"
                                                    add="pozicija, bodovi, timid, drzid" submitlabel="message:submit-label-vozac">
                                        <p:pozicija>
                                            <t:label for="pozicijaUnos"/>:
                                            <t:Textfield t:id="pozicijaUnos"/>
                                        </p:pozicija>
                                        <p:bodovi>
                                            <t:label for="bodoviUnos"/>:
                                            <t:Textfield t:id="bodoviUnos"/>
                                        </p:bodovi>
                                        <p:timid>
                                            <t:label for="timVozacaUnos"/>:
                                            <t:select t:id="timVozacaUnos" validate="required"
                                                      encoder="encoderTima" model="timoviSezoneUnosVozaca"/>
                                        </p:timid>
                                        <p:drzid>
                                            <t:label for="drzavaVozaca"/>:
                                            <t:select t:id="drzavaVozaca" validate="required"
                                                      encoder="encoderDrzave" model="drzave"/>
                                        </p:drzid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetVozaca" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaVozaca">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaTim">
                                <t:zone t:id="zonaFormeUnosIzmenaTima">
                                    <t:beaneditform t:id="unosIzmenaTima" object="tim" exclude="id"
                                                    reorder="nazivTima, logoTima, drzid, pozicija, bodovi"
                                                    add="pozicija, bodovi, drzid" submitlabel="message:submit-label-tim">
                                        <p:pozicija>
                                            <t:label for="pozicijaTimaUnos"/>:
                                            <t:Textfield t:id="pozicijaTimaUnos" validate="required"/>
                                        </p:pozicija>
                                        <p:bodovi>
                                            <t:label for="bodoviTimaUnos"/>:
                                            <t:Textfield t:id="bodoviTimaUnos" validate="required"/>
                                        </p:bodovi>
                                        <p:drzid>
                                            <t:label for="drzavaTima"/>:
                                            <t:select t:id="drzavaTima" validate="required"
                                                      encoder="encoderDrzave" model="drzave"/>
                                        </p:drzid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetTima" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaTima">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaVelikaNagrada">
                                <t:zone t:id="zonaFormeUnosIzmenaVelikeNagrade">
                                    <t:beaneditform t:id="unosIzmenaVelikeNagrade" object="velikaNagrada" exclude="id"
                                                    reorder="nazivVn, datumPocetka, datumZavrsetka, vozid, drzid"
                                                    add="vozid, drzid" submitlabel="message:submit-label-velikanagrada">
                                        <p:vozid>
                                            <t:label for="vozacVelikeNagradeUnos"/>:
                                            <t:select t:id="vozacVelikeNagradeUnos" 
                                                      encoder="encoderVozaca" model="vozaciSezone"/>
                                        </p:vozid>
                                        <p:drzid>
                                            <t:label for="drzavaVelikeNagrade"/>:
                                            <t:select t:id="drzavaVelikeNagrade" 
                                                      encoder="encoderDrzave" model="drzave"/>
                                        </p:drzid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetVelikeNagrade" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaVelikeNagrade">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaTrening">
                                <t:zone t:id="zonaFormeUnosIzmenaTreninga">
                                    <t:beaneditform t:id="unosIzmenaTreninga" object="trening" exclude="id"
                                                    reorder="nazivTreninga, vremeTreninga, sat, minut, vnid"
                                                    add="sat, minut, vnid" submitlabel="message:submit-label-trening">
                                        <p:sat>
                                            <t:label for="satTreninga"/>
                                            <t:Textfield t:id="satTreninga" value="satTre" validate="required,max=23,min=0"/>
                                        </p:sat>
                                        <p:minut>
                                            <t:label for="minutTreninga"/>
                                            <t:Textfield t:id="minutTreninga" value="minutTre" validate="required, max=59,min=0"/>
                                        </p:minut>
                                        <p:vnid>
                                            <t:label for="velikaNagradaTreninga"/>:
                                            <t:select t:id="velikaNagradaTreninga" validate="required"
                                                      encoder="encoderVelikeNagrade" model="velikeNagradeSezone"/>
                                        </p:vnid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetTreninga" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaTreninga">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaKvalifikacije">
                                <t:zone t:id="zonaFormeUnosIzmenaKvalifikacija">
                                    <t:beaneditform t:id="unosIzmenaKvalifikacija" object="kvalifikacije" exclude="id"
                                                    reorder="nazivKvalifikacija, vremeKvalifikacija, sat, minut, vnid"
                                                    add="sat, minut, vnid" submitlabel="message:submit-label-kvalifikacije">
                                        <p:sat>
                                            <t:label for="satKvalifikacija"/>
                                            <t:Textfield t:id="satKvalifikacija" value="satKv" validate="required,max=23,min=0"/>
                                        </p:sat>
                                        <p:minut>
                                            <t:label for="minutKvalifikacija"/>
                                            <t:Textfield t:id="minutKvalifikacija" value="minutKv" validate="required, max=59,min=0"/>
                                        </p:minut>
                                        <p:vnid>
                                            <t:label for="velikaNagradaKvalifikacija"/>:
                                            <t:select t:id="velikaNagradaKvalifikacija" validate="required"
                                                      encoder="encoderVelikeNagrade" model="velikeNagradeSezone"/>
                                        </p:vnid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetKvalifikacija" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaKvalifikacija">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaTrka">
                                <t:zone t:id="zonaFormeUnosIzmenaTrke">
                                    <t:beaneditform t:id="unosIzmenaTrke" object="trka" exclude="id"
                                                    reorder="nazivTrke, najbrziKrug, vremeTrke, sat, minut, vnid"
                                                    add="sat, minut, vnid" submitlabel="message:submit-label-trka">
                                        <p:sat>
                                            <t:label for="satTrke"/>
                                            <t:Textfield t:id="satTrke" value="satTrk" validate="required,max=23,min=0"/>
                                        </p:sat>
                                        <p:minut>
                                            <t:label for="minutTrke"/>
                                            <t:Textfield t:id="minutTrke" value="minutTrk" validate="required, max=59,min=0"/>
                                        </p:minut>
                                        <p:vnid>
                                            <t:label for="velikaNagradaTrke"/>:
                                            <t:select t:id="velikaNagradaTrke" validate="required"
                                                      encoder="encoderVelikeNagrade" model="velikeNagradeSezone"/>
                                        </p:vnid>
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                                <t:actionlink t:id="resetTrke" class="btn btn-primary btn-large"
                                t:zone="zonaFormeUnosIzmenaTrke">
                                    ${message:reset-label}
                                </t:actionlink>
                            </t:if>
                            <t:if test="UnosFormaPozicioniRezultat">
                                <t:zone t:id="zonaFormeIzborPozicioniRezultatUnos">
                                    <t:form t:id="izborPozicioniRezultatUnos" t:zone="zonaUnosaPozicionogRezultata">
                                        <t:label for="izborVelikeNagradePR"/>    
                                        <t:select t:id="izborVelikeNagradePR" t:model="velikeNagradeSezone" 
                                                  encoder="encoderVelikeNagrade" validate="required"/>
                                        <t:RadioGroup value="treningIliKvalifikacijeRezultat" validate="required">
                                            <t:Radio t:id="radioT" value="literal:T" label="message:izborTK-trening-label" type="radio" name="group"/><t:label for="radioT"/><br/>
                                            <t:Radio t:id="radioK" value="literal:K" label="message:izborTK-kvalifikacije-label" type="radio" name="group"/><t:label for="radioK"/><br/>
                                        </t:RadioGroup>
                                        <t:submit class="btn btn-large btn-primary" value="message:izaberi-podatke-label"/>
                                    </t:form>
                                </t:zone>
                                <t:zone t:id="zonaUnosaPozicionogRezultata">
                                    <t:if test="UnosPozicionogRezultata">
                                        <t:zone t:id="zonaFormeUnosIzmenaPozicionogRezultata">
                                            <t:if test="UnosTreningRezultata">
                                                <t:beaneditform t:id="unosIzmenaTreningRezultata" object="pozicioniRezultat" exclude="id"
                                                        reorder="vozid, pozicijaPr, vremeRezultata, treid"
                                                        add="vozid, treid" submitlabel="message:submit-label-pozicioniRezultat">
                                                    <p:vozid>
                                                        <t:label for="vozacTreningRezultata"/>:
                                                        <t:select t:id="vozacTreningRezultata" validate="required"
                                                                  encoder="encoderVozaca" model="vozaciSezone"/>
                                                    </p:vozid>
                                                    <p:treid>
                                                        <t:label for="treningVelikeNagrade"/>:
                                                        <t:select t:id="treningVelikeNagrade" validate="required"
                                                                  encoder="encoderTreninga" model="treninziVelikeNagradeUnos"/>
                                                    </p:treid>
                                                    <t:errors/>            
                                                </t:beaneditform>
                                            </t:if>
                                            <t:if test="!UnosTreningRezultata">
                                                <t:beaneditform t:id="unosIzmenaKvalifikacionogRezultata" object="pozicioniRezultat" exclude="id"
                                                        reorder="vozid, pozicijaPr, vremeRezultata, kvaid"
                                                        add="vozid, kvaid" submitlabel="message:submit-label-pozicioniRezultat">
                                                    <p:vozid>
                                                        <t:label for="vozacKvalifikacionogRezultata"/>:
                                                        <t:select t:id="vozacKvalifikacionogRezultata" validate="required"
                                                                  encoder="encoderVozaca" model="vozaciSezone"/>
                                                    </p:vozid>
                                                    <p:kvaid>
                                                        <t:label for="kvalifikacijeVelikeNagrade"/>:
                                                        <t:select t:id="kvalifikacijeVelikeNagrade" validate="required"
                                                                  encoder="encoderKvalifikacija" model="kvalifikacijeVelikeNagradeUnos"/>
                                                    </p:kvaid>
                                                    <t:errors/>            
                                                </t:beaneditform>
                                            </t:if>
                                        </t:zone>
                                        <t:actionlink t:id="resetPozicionogRezultata"
                                                      class="btn btn-primary btn-large"
                                        t:zone="zonaFormeUnosIzmenaPozicionogRezultata">
                                            ${message:reset-label}
                                        </t:actionlink>
                                    </t:if>
                                </t:zone>
                            </t:if>
                            <t:if test="UnosFormaRezultatTrke">
                                <t:zone t:id="zonaFormeIzborTrkackiRezultatUnos">
                                    <t:form t:id="izborTrkackiRezultatUnos" t:zone="zonaFormeUnosIzmenaTrkackogRezultata">
                                        <t:label for="izborVelikeNagradeTR"/>    
                                        <t:select t:id="izborVelikeNagradeTR" t:model="velikeNagradeSezone" 
                                                  encoder="encoderVelikeNagrade" validate="required" onchange="this.form.submit()"/>
                                        <t:submit class="btn btn-large btn-primary" value="message:izaberi-podatke-label"/>
                                    </t:form>
                                </t:zone>
                                <t:if test="UnosTrkackogRezultata">
                                    <t:zone t:id="zonaFormeUnosIzmenaTrkackogRezultata">
                                        <t:beaneditform t:id="unosIzmenaTrkackogRezultata" 
                                                object="trkackiRezultat" exclude="id"
                                                reorder="vozid, pozicijaTr, ostvareniRezultat, trkid"
                                                add="vozid, trkid" submitlabel="message:submit-label-trkackiRezultat">
                                            <p:vozid>
                                                <t:label for="vozacTrkackogRezultata"/>:
                                                <t:select t:id="vozacTrkackogRezultata" validate="required"
                                                          encoder="encoderVozaca" model="vozaciSezone"/>
                                            </p:vozid>
                                            <p:trkid>
                                                <t:label for="trkaVelikeNagrade"/>:
                                                <t:select t:id="trkaVelikeNagrade" validate="required"
                                                          encoder="encoderTrka" model="trkeVelikeNagradeUnos"/>
                                            </p:trkid>
                                            <t:errors/>            
                                        </t:beaneditform>
                                    </t:zone>
                                    <t:actionlink t:id="resetTrkackogRezultata" 
                                                  class="btn btn-primary btn-large"
                                    t:zone="zonaFormeUnosIzmenaTrkackogRezultata">
                                        ${message:reset-label}
                                    </t:actionlink>
                                </t:if>
                            </t:if>
                            <t:if test="IzmenaFormaKlasifikacijaVozac">
                                <t:zone t:id="zonaFormeIzmenaKlasifikacijeVozaca">
                                    <t:beaneditform t:id="izmenaKlasifikacijeVozaca" 
                                                    object="klasifikacijaVozac" exclude="id"
                                                    reorder="pozicijaK, bodovi" 
                                                    submitlabel="message:submit-label-vozac-klasifikacija">
                                        <t:errors/>            
                                    </t:beaneditform>
                                </t:zone>
                            </t:if>
                            <t:if test="IzmenaFormaKlasifikacijaTim">
                                <t:zone t:id="zonaFormeIzmenaKlasifikacijeTima">
                                    <t:beaneditform t:id="izmenaKlasifikacijeTima" 
                                                    object="klasifikacijaTim" exclude="id"
                                                    reorder="pozicijaK, bodovi" submitlabel="message:submit-label-tim-klasifikacija">
